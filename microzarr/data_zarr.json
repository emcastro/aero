{
  "zarr_format":3,
  "node_type":"array",
  "shape":[
    "${width}",
    "${height}"
  ],
  "data_type":"int16",
  "chunk_grid":{
    "name":"regular",
    "configuration":{
      "chunk_shape":[
        "${chunk_width}",
        "${chunk_height}"
      ]
    }
  },
  "chunk_key_encoding":{
    "name":"default",
    "configuration":{
      "separator":"\/"
    }
  },
  "fill_value":"${fill_value}",
  "attributes":{
    "AREA_OR_POINT":"Point",
    "_CRS":{
      "projjson":{
        "datum":{
          "ellipsoid":{
            "name":"WGS 84"
          }
        },
        "coordinate_system":{
          "subtype":"ellipsoidal",
          "axis":[
            {
              "direction":"north",
              "unit":"degree"
            },
            {
              "direction":"east",
              "unit":"degree"
            }
          ]
        },
        "id":{
          "authority":"EPSG",
          "code":4326
        }
      }
    }
  },
  "dimension_names":[
    "Y",
    "X"
  ]
}